---
import Logo from "./Logo.astro";
import Section from "./Section.astro";
---

<Section isDark>
  <div class="container"><Logo /></div>
</Section>
<div class="footer"></div>

<style>
  .container {
    display: flex;
    padding: 3.5rem 0;
    justify-content: center;
    align-items: center;
  }
  .footer {
    width: 100vw;
    height: 8rem;
    background-image: url("/footer.png");
    background-repeat: repeat no-repeat;
  }
</style>
